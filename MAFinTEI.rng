   <define name="tei_att.referring.attributes">
      <ref name="tei_att.referring.attribute.referringMode"/>
      <ref name="tei_att.referring.attribute.from"/>
      <ref name="tei_att.referring.attribute.to"/>
   </define>
   <define name="tei_att.referring.attribute.referringMode">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="referringMode"
                    a:defaultValue="pointer">
            <a:documentation>specifies whether the span/segment edges are identified as numeric offsets or as pointers (URIs or fragment identifiers). The default value of this attribute is pointer and it is optional in cases where edges are identified by pointing. Otherwise, it has to be present.</a:documentation>
            <choice>
               <value>pointer</value>
               <a:documentation>edges are identified by pointing at elements (e.g. <code xmlns="http://www.w3.org/1999/xhtml">&lt;w&gt;</code>, <code xmlns="http://www.w3.org/1999/xhtml">&lt;c&gt;</code> or <code xmlns="http://www.w3.org/1999/xhtml">&lt;seg&gt;</code>) that can be pointed at by means of an URI (which most often means that they have <code xmlns="http://www.w3.org/1999/xhtml">@xml:id</code> attributes defined).</a:documentation>
               <value>id</value>
               <a:documentation>edges are identified by pointing at existing elements (e.g. <code xmlns="http://www.w3.org/1999/xhtml">&lt;w&gt;</code>, <code xmlns="http://www.w3.org/1999/xhtml">&lt;c&gt;</code> or <code xmlns="http://www.w3.org/1999/xhtml">&lt;seg&gt;</code>) that carry <code xmlns="http://www.w3.org/1999/xhtml">@xml:id</code> attributes; this value ensures ID validation -- in this respect, it is analogous to the IDREF data types of DTDs.</a:documentation>
               <value>character</value>
               <a:documentation>edges are identified by numeric offsets (non-negative integers); note that a separate project-specific convention regulates the value of the starting index (0 or 1).</a:documentation>
               <value>icp</value>
               <a:documentation>"icp" stands for "inter-character point". Edges are identified by numeric offsets (non-negative integers) identifying inter-character points. In e.g. the string "point", inter-character points are marked by vertical lines: |p|o|i|n|t| (by convention, there is an icp before the first character and after the last one); note that a separate project-specific convention regulates the value of the starting index (0 or 1). ISO LAF and ISO MAF advocate 0 as the start index.</a:documentation>
               <value>byte</value>
               <a:documentation>edges are identified by numeric offsets (non-negative integers) addressing bytes. Note that this low-level indexing type ignores XML or any other structuring.</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tei_att.referring.attribute.from">
      <optional>
         <attribute name="from">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the starting point of a sequence of characters or bytes, or of elements that can be pointed at with a URI.</a:documentation>
            <choice>
               <data type="nonNegativeInteger"/>
               <data type="anyURI"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tei_att.referring.attribute.to">
      <optional>
         <attribute name="to">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the end-point of a sequence of characters or bytes, or of elements that can be pointed at with a URI.</a:documentation>
            <choice>
               <data type="nonNegativeInteger"/>
               <data type="anyURI"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.referring-default_mode-constraint-rule-8">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[local-name() = ('span')][not(@referringMode) and @from and @to]">
                  <sch:assert test="@from castable as xsd:anyURI">The default form of @from is a URI</sch:assert>
                  <sch:assert test="@to castable as xsd:anyURI">The default form of @to is a URI</sch:assert>
                </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.referring-two-and-three-match-one-constraint-rule-9">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[@referringMode eq 'pointer']">
                  <sch:assert test="@from castable as xsd:anyURI">When @referringMode is 'pointer',
                    @from must be a URI</sch:assert>
                  <sch:assert test="@to castable as xsd:anyURI">When @referringMode is 'pointer', @to
                    must be a URI</sch:assert>
                </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.referring-two-and-three-match-one-constraint-rule-10">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[@referringMode eq 'id']">
                  <sch:assert test="id(substring(@from,2))">When @referringMode is 'id',
                    @from must be pointing at an existing local target</sch:assert>
                  <sch:assert test="id(substring(@to,2))">When @referringMode is 'id',
                    @to must be pointing at an existing local target</sch:assert>
                </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.referring-two-and-three-match-one-constraint-rule-11">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[@referringMode = ('character','icp','byte')]">
                  <sch:assert test="@from castable as xsd:int">When @referringMode is
                    '<sch:value-of select="@referringMode"/>', @from must be a non-negative integer</sch:assert>
                  <sch:assert test="@to castable as xsd:int">When @referringMode is
                    '<sch:value-of select="@referringMode"/>', @to must be a non-negative integer</sch:assert>
                </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.referring-tofrom-constraint-rule-12">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[local-name() = ('span')][@to or @from]">
                  <sch:report test="contains(normalize-space(@to),' ') or contains(normalize-space(@from),' ')">The attributes @to and @from on <sch:name/> may each contain only a single
                    value</sch:report>
                </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.referring-tonotfrom-constraint-rule-13">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[local-name() = ('span')][@to]">
                  <sch:report test="@to and not(@from)">If @to is supplied on <sch:name/>, @from must
                    be supplied as well</sch:report>
                </sch:rule>
   </pattern>
   <start>
      <ref name="tei_TEI"/>
   </start>
</grammar>
