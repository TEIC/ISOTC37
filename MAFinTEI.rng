   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.offset-endPosnotstartPos-constraint-rule-9">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[@endPos]">
         <sch:assert test="@startPos">If @endPos is supplied on <sch:name/>, @startPos must
                    be supplied as well</sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.offset-startPosnotendPos-constraint-rule-10">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[@startPos]">
         <sch:assert test="@endPos">If @startPos is supplied on
                    <sch:name/>, @endPos must be supplied as well</sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="MAFinTEI-att.offset-offsetBaseObligatory-constraint-rule-11">
      <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="*[@endPos]">
         <sch:assert test="ancestor-or-self::*[@offsetBase]">offset attributes require @offsetBase to be defined in the same element or its
                    ancestor</sch:assert>
      </sch:rule>
   </pattern>
